<bindings xmlns:xs="http://www.w3.org/2001/XMLSchema" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd" 
          version="2.1" 
          xmlns="http://java.sun.com/xml/ns/jaxb">
  <globalBindings fixedAttributeAsConstantProperty="true"
                  typesafeEnumMemberName="generateError"
                  underscoreBinding="asCharInWord">
  </globalBindings>
  <bindings schemaLocation="../resources/schemas/cpsv-ap_xml_schema_v0.01.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="eu.toop.edm.jaxb.cpsv" />
    </schemaBindings>
  </bindings>
  <bindings schemaLocation="../resources/schemas/cpsv-ap_xml_schema_v0.01.helper.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="eu.toop.edm.jaxb.cpsv.helper" />
    </schemaBindings>
  </bindings>

  <bindings schemaLocation="../resources/schemas/cccev-2.0.0.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="eu.toop.edm.jaxb.cccev" />
      <nameXmlTransform>
        <typeName prefix="CCCEV"/>
      </nameXmlTransform>
    </schemaBindings>
  </bindings>
  <bindings schemaLocation="../resources/schemas/CV-Agent.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="eu.toop.edm.jaxb.cv.agent" />
    </schemaBindings>
  </bindings>
  <bindings schemaLocation="../resources/schemas/CV-CommonAggregateComponents.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="eu.toop.edm.jaxb.cv.cac" />
    </schemaBindings>
    <bindings node="//xs:attributeGroup[@name='ThingAttributes']">
      <bindings node="./xs:attribute[@name='versionId']">
        <property name="VersionIdAttr"/>
      </bindings>
    </bindings>
  </bindings>
  <bindings schemaLocation="../resources/schemas/CV-CommonBasicComponents.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="eu.toop.edm.jaxb.cv.cbc" />
    </schemaBindings>
  </bindings>
  <bindings schemaLocation="../resources/schemas/CV-DataTypes.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="eu.toop.edm.jaxb.cv.dt" />
    </schemaBindings>
  </bindings>
  <bindings schemaLocation="../resources/schemas/dcat-ap.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="eu.toop.edm.jaxb.dcatap" />
      <nameXmlTransform>
        <typeName prefix="DCatAP"/>
      </nameXmlTransform>
    </schemaBindings>
  </bindings>
</bindings>
