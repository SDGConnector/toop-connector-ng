<bindings xmlns:xs="http://www.w3.org/2001/XMLSchema" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd" 
          version="2.1" 
          xmlns="http://java.sun.com/xml/ns/jaxb">
  <globalBindings typesafeEnumMaxMembers="2000" typesafeEnumMemberName="generateError">
  </globalBindings>
  <bindings schemaLocation="../resources/schemas/tc-rest.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="eu.toop.connector.api.rest" />
    </schemaBindings>
  </bindings>
  <bindings schemaLocation="../resources/schemas/tc-simulator.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="eu.toop.connector.api.simulator" />
    </schemaBindings>
  </bindings>
</bindings>
